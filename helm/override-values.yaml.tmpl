hosts:
    host: ${HOST}

db:
    uri: ${DATABASE_URL}

redis:
    nameOverride: ${REDIS_HOST}
    password: ${REDIS_PASSWORD}
    master:
        port: ${REDIS_PORT}

image:
    registry: ${CI_REGISTRY}
    username: ${CI_DEPLOY_USER}
    password: ${CI_DEPLOY_PASSWORD}
    location: ${CI_REGISTRY}/${CI_PROJECT_PATH}:${CI_COMMIT_HASH}

environment_name: ${CI_ENVIRONMENT_NAME}
